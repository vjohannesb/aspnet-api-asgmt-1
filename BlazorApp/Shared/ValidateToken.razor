@code {
    public bool TokenValidated { get; private set; }

    protected override async Task OnInitializedAsync()
    {
        var response = await apiService.SendToAPIAsync(HttpMethod.Post, "https://localhost:44330/api/admin/validate", auth: true);
        TokenValidated = response.IsSuccessStatusCode;
        if (!TokenValidated)
            navigationManager.NavigateTo("/signin", forceLoad: true);
    }
}
